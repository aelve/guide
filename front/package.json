{
  "name": "@vert/vue-ssr-template",
  "version": "1.0.0",
  "description": "Vue project template with SSR, Vert and TypeScript support.",
  "private": true,
  "scripts": {
    "client:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.client.conf.js",
    "client:build": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.conf.js",
    "server:dev": "cross-env NODE_ENV=development nodemon ./server/bin.js --watch server/**/*.js",
    "server:build": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.conf.js",
    "build-all": "npm run client:build && npm run server:build",
    "start:dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "start:prod": "cross-env NODE_ENV=production node server/bin.js",
    "test": "testcafe chrome client/page/*.test.js --app \"npm run start:dev\" --app-incit-delay 10000"
  },
  "dependencies": {
    "@junyiz/koa-proxy-pass": "^1.2.1",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-proxies": "^0.7.0",
    "koa-proxy": "^0.9.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-static-server": "^1.3.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "reflect-metadata": "^0.1.12",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-meta": "^1.5.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "https://github.com/aelve/vue-server-renderer",
    "vuetify": "^1.2.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@types/lodash": "^4.14.116",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "material-design-icons-iconfont": "^3.0.3",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "postcss-loader": "^2.1.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "testcafe": "^0.22.0",
    "testcafe-vue-selectors": "^3.0.0",
    "ts-loader": "^4.4.2",
    "typescript": "^2.9.2",
    "tslint": "^5.11.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
